@model DashboardSummary
<div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
    <div class="dashboard-stat2 bordered">
        <div class="display">
            <div class="number">
                <h3 class="font-red-haze">
                    <span data-counter="counterup" data-value="@Model.Feedback">0</span>
                </h3>
                <small>NEW FEEDBACKS</small>
            </div>
            <div class="icon">
                <i class="icon-like"></i>
            </div>
        </div>
        <div class="progress-info">
            <div class="progress">
                <span style="width: @Model.FeedbackChange%;" class="progress-bar progress-bar-success red-haze">
                    <span class="sr-only">@Model.FeedbackChange% change</span>
                </span>
            </div>
            <div class="status">
                <div class="status-title"> change </div>
                <div class="status-number"> @Model.FeedbackChange% </div>
            </div>
        </div>
    </div>
</div>