
@{
    Layout = "_BlankLayout";
    ViewData["Title"] = "User Lock Screen 2";
}

@section links{
    <link href="~/pages/css/lock-2.min.css" rel="stylesheet" type="text/css" />
}

<div class="page-lock">
    <div class="page-logo">
        <a class="brand" href="index.html">
            <img src="~/pages/img/logo-big.png" alt="logo" />
        </a>
    </div>
    <div class="page-body">
        <img class="page-lock-img" src="~/pages/media/profile/profile.jpg" alt="">
        <div class="page-lock-info">
            <h1>Bob Nilson</h1>
            <span class="email"> bob@keenthemes.com </span>
            <span class="locked"> Locked </span>
            <form class="form-inline" action="index.html">
                <div class="input-group input-medium">
                    <input type="text" class="form-control" placeholder="Password">
                    <span class="input-group-btn">
                        <button type="submit" class="btn green icn-only">
                            <i class="m-icon-swapright m-icon-white"></i>
                        </button>
                    </span>
                </div>
                <!-- /input-group -->
                <div class="relogin">
                    <a href="login.html"> Not Bob Nilson ? </a>
                </div>
            </form>
        </div>
    </div>
    <div class="page-footer-custom"> 2014 &copy; Metronic. Admin Dashboard Template. </div>
</div>

@section scripts{
    <script src="~/global/plugins/backstretch/jquery.backstretch.min.js" type="text/javascript"></script>
    <script src="~/pages/scripts/lock-2.min.js" type="text/javascript"></script>
}
