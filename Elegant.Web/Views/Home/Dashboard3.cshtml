@{
    ViewData["Title"] = "Dashboard3";
}

<!-- BEGIN PAGE HEAD-->
<div class="page-head">
    <!-- BEGIN PAGE TITLE -->
    <div class="page-title">
        <h1>
            Admin Dashboard 3
            <small>statistics, charts, recent events and reports</small>
        </h1>
    </div>
    <!-- END PAGE TITLE -->
    <partial name="_ToolBar" />
</div>
<!-- END PAGE HEAD-->
<!-- BEGIN PAGE BREADCRUMB -->
<ul class="page-breadcrumb breadcrumb">
    <li>
        <a asp-action="Index">Home</a>
        <i class="fa fa-circle"></i>
    </li>
    <li>
        <span class="active">Dashboard</span>
    </li>
</ul>
<!-- END PAGE BREADCRUMB -->
<!-- BEGIN PAGE BASE CONTENT -->
<div class="row widget-row">
    <partial name="_Landscape3" model="Model" />
</div>
<div class="row">
    <partial name="_AmChart3" />
</div>
<div class="row">
    <partial name="_Comments" />
</div>
<div class="row">
    <partial name="_Activities" />
</div>
<div class="row">
    <partial name="_RecentActivities" />
</div>
<div class="row">
    <partial name="_GeneralStats" />
</div>
<div class="row">
    <partial name="_RegionalStats" />
</div>
<div class="row">
    <partial name="_RecentUser" />
</div>
@section scripts{
    <partial name="_AmchartScriptsPartial" />
    <partial name="_FlotScriptsPartial" />
    <partial name="_VmapScriptsPartial" />
    <script src="~/pages/scripts/dashboard.min.js" type="text/javascript"></script>
    <script>
        $(document).ready(function () {
            $('#clickmewow').click(function () {
                $('#radio1003').attr('checked', 'checked');
            });
        })
    </script>
}