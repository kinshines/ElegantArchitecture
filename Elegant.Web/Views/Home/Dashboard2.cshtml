@{
    ViewData["Title"] = "Dashboard2";
}

@section links{
    <link href="~/global/plugins/bootstrap-daterangepicker/daterangepicker.min.css" rel="stylesheet" />
    <link href="~/global/plugins/morris/morris.css" rel="stylesheet" />
    <link href="~/global/plugins/fullcalendar/fullcalendar.min.css" rel="stylesheet" />
}

<!-- BEGIN PAGE HEAD-->
<div class="page-head">
    <!-- BEGIN PAGE TITLE -->
    <div class="page-title">
        <h1>
            Admin Dashboard
            <small>statistics, charts, recent events and reports</small>
        </h1>
    </div>
    <!-- END PAGE TITLE -->
    <partial name="_ToolBar" />
</div>
<!-- END PAGE HEAD-->
<!-- BEGIN PAGE BREADCRUMB -->
<ul class="page-breadcrumb breadcrumb">
    <li>
        <a asp-action="Index">Home</a>
        <i class="fa fa-circle"></i>
    </li>
    <li>
        <span class="active">Dashboard</span>
    </li>
</ul>
<!-- END PAGE BREADCRUMB -->
<!-- BEGIN PAGE BASE CONTENT -->
<div class="row">
    <partial name="_Landscape2" model="Model" />
</div>
<div class="row">
    <partial name="_AmChart2" />
</div>
<div class="row">
    <partial name="_Comments" />
</div>
<div class="row">
    <partial name="_Activities" />
</div>
<div class="row">
    <partial name="_RecentActivities" />
</div>
<div class="row">
    <partial name="_GeneralStats" />
</div>
<div class="row">
    <partial name="_RegionalStats" />
</div>
<div class="row">
    <partial name="_Chat" />
</div>
<div class="row">
    <partial name="_RecentUser" />
</div>
@section scripts{
    <script src="~/global/plugins/moment.min.js" type="text/javascript"></script>
    <script src="~/global/plugins/bootstrap-daterangepicker/daterangepicker.min.js" type="text/javascript"></script>
    <script src="~/global/plugins/morris/morris.min.js" type="text/javascript"></script>
    <script src="~/global/plugins/morris/raphael-min.js" type="text/javascript"></script>
    <script src="~/global/plugins/counterup/jquery.waypoints.min.js" type="text/javascript"></script>
    <script src="~/global/plugins/counterup/jquery.counterup.min.js" type="text/javascript"></script>
    <script src="~/global/plugins/fullcalendar/fullcalendar.min.js" type="text/javascript"></script>
    <script src="~/global/plugins/horizontal-timeline/horizontal-timeline.js" type="text/javascript"></script>
    <partial name="_AmchartScriptsPartial" />
    <partial name="_FlotScriptsPartial" />
    <script src="~/pages/scripts/dashboard.min.js" type="text/javascript"></script>
}